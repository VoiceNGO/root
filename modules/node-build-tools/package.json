{
  "name": "node-build-tools",
  "version": "0.0.1",
  "author": "Mark Kahn <m@voice.ngo>",
  "private": true,
  "exports": {
    "./*": "./build/*.js",
    "./build-utils/*": "./build/build-utils/*.js",
    "./file-builders/*": "./build/file-builders/*.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "build/*"
      ]
    }
  },
  "sideEffects": [
    "*.se.ts"
  ],
  "bin": {
    "build": "./bin/build.cjs",
    "jest": "./bin/test.cjs",
    "test": "./bin/test.cjs",
    "tsc": "./bin/build.cjs"
  },
  "devDependencies": {
    "@types/voice": "workspace:*",
    "enhanced-resolve": "^5.8.3",
    "typescript": "next"
  },
  "dependencies": {
    "jest": "^27.4.4"
  }
}
